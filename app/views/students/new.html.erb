<%= form_tag students_path do %>

  <h1>Student Form</h1>

  <div class="">
    <label>Student first name:</label>
    <%= text_field_tag :'first_name'  %>
  </div>

  <div class="">
    <label>Student last name:</label>
    <%= text_field_tag :'last_name'  %>
  </div>

  <%= submit_tag "Submit Student" %>

<% end %>
